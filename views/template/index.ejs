<% include head %>

<% include slider %>

<% for (var i=0;i<data.docs.length;i++) {%>
        <div class="col s3">
          <div class="card">
            <div class="card-content">
              <span class="card-title"><%=data.docs[i].name%></span>
              <div class="card-image">
              <img  src="/<%=data.docs[i].gambar%>" alt="<%=data.docs[i].name%>" width="250" height="100" >
              </div>
              <p><%=data.docs[i].detail%></p>
            </div>
            <div class="card-action valign center">
              <a align="center" href="/detail/<%=data.docs[i].id%>">Detail</a>
              <a align="center" href="#">Order</a>
            </div>
          </div>
        </div>
<% } %>
<div class="col s12">
<ul class="pagination valign center">
<% if (data.pages=1){ %>
<% for (var i=1;i<data.pages+1;i++) {%>
<li class="waves-effect"><a><%=i%></a><a href="/?page=<%=i+1%>"><h6><b>></b></h6></a></li>
<% } %>
<% } %>
</ul>
</div>
  <% include foot %>
